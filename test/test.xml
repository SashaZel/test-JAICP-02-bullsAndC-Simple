<test>
    <test-case id="01_Basic_No_Match">
        <q>/start</q>
        <a state="/Start"/>
        <q>ффффФФФААА</q>
        <a state="/NoMatch"/>
    </test-case>
    <test-case id="02_Basic_greeting_No">
        <q>/start</q>
        <a state="/Start"/>
        <q>нет</q>
        <a state="/Start/Agree?/AgreeNo"/>
    </test-case>
    <test-case id="03_Basic_greeting_Yes">
        <q>/start</q>
        <a state="/Start"/>
        <q>да</q>
        <a state="/Start/Agree?/GameStart"/>
    </test-case>
    <test-case id="04_Basic_Game_Start_and_error_handling">
        <q>/start</q>
        <a state="/Start"/>
        <q>да</q>
        <a state="/Start/Agree?/GameStart"/>
        <q>1234556</q>
        <a state="/Start/Agree?/GameStart"/>
        <q>ьььььФФАААуууу</q>
        <a state="/NoMatch"/>
        <q>Правила</q>
        <a state="/Start"/>
    </test-case>
</test>    